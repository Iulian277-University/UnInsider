{
  "name": "nodejs-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "watch": "npm-watch"
  },
  "watch": {
    "start": {
      "patterns": [
        "server.js",
        "app/routes/*.js",
        "app/controllers/*.js"
      ],
      "extensions": "js"
    }
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "dependencies": {
    "bad-words": "^3.0.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "languagedetect": "^2.0.0",
    "natural": "^6.3.0",
    "npm-watch": "^0.11.0",
    "simple-spellchecker": "^1.0.2"
  }
}
